{
  "manifest_version": 3,
  "name": "Math Addict",
  "version": "0.0.2",
  "description": "A basic demo of what MathAddict will be able to do.",

  "icons": {
    "32": "WaveTempIcon.png"
  },


  "action": {
    "default_popup": "index.html"
  },

  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}